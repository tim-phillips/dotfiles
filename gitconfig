[alias]
    b = branch
    s = status
    co = checkout
    ci = commit
    tree = log --graph --decorate --pretty=oneline --abbrev-commit
    last = log -1 HEAD
    find = log --name-status --oneline -G
[user]
    name = Tim Phillips
    email = timphillips@hey.com
[filter "media"]
    clean = git-media-clean %f
    smudge = git-media-smudge %f
[push]
    default = simple
[pull]
    ff = only
[init]
    defaultBranch = main
